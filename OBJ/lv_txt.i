            result += (txt[*i] & 0x3F);
            (*i)++;
        }
         
        else if((txt[*i] & 0xF8) == 0xF0) {
            result = (uint32_t)(txt[*i] & 0x07) << 18;
            (*i)++;

            if((txt[*i] & 0xC0) != 0x80) return 0;  
            result += (uint32_t)(txt[*i] & 0x3F) << 12;
            (*i)++;

