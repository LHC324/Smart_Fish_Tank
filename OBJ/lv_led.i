    lv_led_ext_t * ext = lv_obj_get_ext_attr(led);
    return ext->bright;
}



 










 
static lv_design_res_t lv_led_design(lv_obj_t * led, const lv_area_t * clip_area, lv_design_mode_t mode)
{
    if(mode == LV_DESIGN_COVER_CHK) {
         
        return ancestor_design(led, clip_area, mode);
    }
    else if(mode == LV_DESIGN_DRAW_MAIN) {
         
        lv_led_ext_t * ext       = lv_obj_get_ext_attr(led);

        lv_draw_rect_dsc_t rect_dsc;
        lv_draw_rect_dsc_init(&rect_dsc);
        lv_obj_init_draw_rect_dsc(led, LV_LED_PART_MAIN, &rect_dsc);

         
        rect_dsc.bg_color   = lv_color_mix(rect_dsc.bg_color, ((lv_color_t){{(uint16_t)((0x00 >> 3) & 0x1FU), (uint16_t)((0x00 >> 2) & 0x3FU), (uint16_t)((0x00 >> 3) & 0x1FU)}}), ext->bright);
        rect_dsc.bg_grad_color   = lv_color_mix(rect_dsc.bg_grad_color, ((lv_color_t){{(uint16_t)((0x00 >> 3) & 0x1FU), (uint16_t)((0x00 >> 2) & 0x3FU), (uint16_t)((0x00 >> 3) & 0x1FU)}}), ext->bright);
        rect_dsc.border_color = lv_color_mix(rect_dsc.border_color, ((lv_color_t){{(uint16_t)((0x00 >> 3) & 0x1FU), (uint16_t)((0x00 >> 2) & 0x3FU), (uint16_t)((0x00 >> 3) & 0x1FU)}}), ext->bright);
        rect_dsc.shadow_color = lv_color_mix(rect_dsc.shadow_color, ((lv_color_t){{(uint16_t)((0x00 >> 3) & 0x1FU), (uint16_t)((0x00 >> 2) & 0x3FU), (uint16_t)((0x00 >> 3) & 0x1FU)}}), ext->bright);

        
 
        rect_dsc.shadow_width = ((ext->bright - 120) * rect_dsc.shadow_width) /
                                (255 - 120);
        rect_dsc.shadow_spread = ((ext->bright - 120) * rect_dsc.shadow_spread) /
                                 (255 - 120);

        lv_draw_rect(&led->coords, clip_area, &rect_dsc);
    }
    return LV_DESIGN_RES_OK;
}







 
static lv_res_t lv_led_signal(lv_obj_t * led, lv_signal_t sign, void * param)
{
    lv_res_t res;

     
    res = ancestor_signal(led, sign, param);
    if(res != LV_RES_OK) return res;

    if(sign == LV_SIGNAL_GET_TYPE) {
        lv_obj_type_t * buf = param;
        uint8_t i;
        for(i = 0; i < 8 - 1; i++) {  
            if(buf->type[i] == 0) break;
        }
        buf->type[i] = "lv_led";
    }

    return res;
}
