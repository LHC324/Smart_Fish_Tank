lv_obj_t * lv_led_create(lv_obj_t * par, const lv_obj_t * copy)
{
    ;

     
    lv_obj_t * led = lv_obj_create(par, copy);
    do { if(!((lv_debug_check_null(led)))) { ; lv_debug_log_error("Out of memory", (uint64_t)((uintptr_t)led)); while(1); } } while(0);;
    if(led == 0) return 0;

    if(ancestor_signal == 0) ancestor_signal = lv_obj_get_signal_cb(led);
    if(ancestor_design == 0) ancestor_design = lv_obj_get_design_cb(led);

     
    lv_led_ext_t * ext = lv_obj_allocate_ext_attr(led, sizeof(lv_led_ext_t));
    do { if(!((lv_debug_check_null(ext)))) { ; lv_debug_log_error("Out of memory", (uint64_t)((uintptr_t)ext)); while(1); } } while(0);;
    if(ext == 0) {
        lv_obj_del(led);
        return 0;
    }

    ext->bright = 255;

    lv_obj_set_signal_cb(led, lv_led_signal);
    lv_obj_set_design_cb(led, lv_led_design);

     
    if(copy == 0) {
        lv_obj_set_size(led, (60 / 3), (60 / 3));

        lv_theme_apply(led, LV_THEME_LED);
    }
     
    else {
        lv_led_ext_t * copy_ext = lv_obj_get_ext_attr(copy);
        ext->bright             = copy_ext->bright;

         
        lv_obj_refresh_style(led, LV_OBJ_PART_ALL, 0xFF);
    }

    ;

    return led;
}



 





 
void lv_led_set_bright(lv_obj_t * led, uint8_t bright)
{
    do { if(!((lv_debug_check_null(led)))) { ; lv_debug_log_error("NULL pointer", (uint64_t)((uintptr_t)led)); while(1); } } while(0);;

     
    lv_led_ext_t * ext = lv_obj_get_ext_attr(led);
    if(ext->bright == bright) return;

    if(bright <= 120) bright = 120;
    if(bright >= 255) bright = 255;

    ext->bright = bright;

     
    lv_obj_invalidate(led);
}




 
void lv_led_on(lv_obj_t * led)
{
    do { if(!((lv_debug_check_null(led)))) { ; lv_debug_log_error("NULL pointer", (uint64_t)((uintptr_t)led)); while(1); } } while(0);;

    lv_led_set_bright(led, 255);
}




 
void lv_led_off(lv_obj_t * led)
{
    do { if(!((lv_debug_check_null(led)))) { ; lv_debug_log_error("NULL pointer", (uint64_t)((uintptr_t)led)); while(1); } } while(0);;

    lv_led_set_bright(led, 120);
}




 
void lv_led_toggle(lv_obj_t * led)
{
    do { if(!((lv_debug_check_null(led)))) { ; lv_debug_log_error("NULL pointer", (uint64_t)((uintptr_t)led)); while(1); } } while(0);;

    uint8_t bright = lv_led_get_bright(led);
    if(bright > (120 + 255) >> 1)
        lv_led_off(led);
    else
        lv_led_on(led);
}



 





 
